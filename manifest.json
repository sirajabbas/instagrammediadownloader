{
    "manifest_version": 2,
  
    "name": "Insta Media Downloader",
    "description": "This extension downloads instagram images",
    "version": "1.0",
    "background": {
        "page": "background.html"
      },
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "tabs",
     "background",
     "downloads"
     ]
  }